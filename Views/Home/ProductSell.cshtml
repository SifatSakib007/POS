<div class="p-4">
    @* পণ্য বিক্রয় নিবন্ধন  *@
    <p class="text-center text-xl mb-4">পণ্য বিক্রয় নিবন্ধন</p>

    <div class="font-[sans-serif] bg-white">
        <div class="max-w-4xl w-full h-max rounded-md px-4 py-8 mx-auto">
            <form class="mt-8">
                @* {/* Personal Details */} *@
                <div>
                    <div class="flex flex-col md:grid md:grid-cols-2 gap-4">
                        <div>
                            <label htmlFor="firstName" class="block text-sm text-gray-600">
                                পণ্যের নাম বাছাই করুন
                            </label>
                            <select id="firstName" class="px-4 py-3 bg-gray-100 focus:bg-white text-gray-800 w-full text-sm rounded-md focus:outline-blue-600">
                                <option value="">পণ্যের নাম বাছাই করুন</option>
                                <option value="john">ধান</option>
                                <option value="jane">গম</option>
                                <option value="michael">আলু</option>
                                <option value="emily">পটল</option>
                            </select>
                        </div>

                        <div>
                            <label htmlFor="lastName" class="block text-sm text-gray-600">
                                পণ্যের কেনা মূল্য
                            </label>
                            <input type="number" id="productbuyrate" placeholder="সংখ্যায় লিখুন" class="px-4 py-3 bg-gray-100 focus:bg-white text-gray-800 w-full text-sm rounded-md focus:outline-blue-600" />
                        </div>

                        <div>
                            <label htmlFor="email" class="block text-sm text-gray-600">
                                স্টক
                            </label>
                            <input type="number" id="stock" placeholder="সংখ্যায় লিখুন" class="px-4 py-3 bg-gray-100 focus:bg-white text-gray-800 w-full text-sm rounded-md focus:outline-blue-600" />
                        </div>

                        <div>
                            <label htmlFor="phone" class="block text-sm text-gray-600">
                                কতটুকু কিনবে পণ্য
                            </label>
                            <input type="number" id="quantity" placeholder="সংখ্যায় লিখুন" class="px-4 py-3 bg-gray-100 focus:bg-white text-gray-800 w-full text-sm rounded-md focus:outline-blue-600" />
                        </div>

                        <div>
                            <label htmlFor="lastName" class="block text-sm text-gray-600">
                                পণ্যের বিক্রয় মূল্য
                            </label>
                            <input type="number" id="productsellate" placeholder="সংখ্যায় লিখুন" class="px-4 py-3 bg-gray-100 focus:bg-white text-gray-800 w-full text-sm rounded-md focus:outline-blue-600" />
                        </div>

                        <div class="text-xl font-bold ml-6">
                            <label htmlFor="lastName" class="block text-sm text-gray-600">
                                মোট মুল্য
                            </label>
                            <p>১০০ টাকা</p>
                        </div>
                    </div>
                </div>


                @* {/* Shipping Address */} *@
                <div class="mt-8">


                    @* {/* Buttons */} *@
                    <div class="flex gap-4 max-md:flex-col mt-8">
                        <button type="button"
                                class="rounded-md px-6 py-3 w-full text-sm tracking-wide bg-transparent hover:bg-gray-100 border border-gray-300 text-gray-800 max-md:order-1">
                            মুছে ফেলুন
                        </button>
                        <button type="submit"
                                class="rounded-md px-6 py-3 w-full text-sm tracking-wide bg-blue-600 hover:bg-blue-700 text-white">
                            যুক্ত করুন
                        </button>
                    </div>
                </div>
            </form>
        </div>
    </div>
